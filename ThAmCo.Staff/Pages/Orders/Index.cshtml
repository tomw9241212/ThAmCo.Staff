@page
@model ThAmCo.Staff.Pages.Orders.IndexModel
@{
}

<h3>Orders</h3>

<table>
    @foreach (var order in Model.orders) {
        <tr>
            <td>@order.Id</td>
            <td>@order.Status</td>
            <td>@order.CustomerId</td>
            <td>@order.SubmittedDate</td>
        </tr>
    }
</table>

